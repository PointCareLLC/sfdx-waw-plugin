{
  "name": "sfdx-waw-plugin",
  "description": "A plugin for the Salesforce CLI built by Wade Wegner and containing a lot of helpful commands.",
  "version": "0.9.5",
  "author": "Wade Wegner @WadeWegner",
  "bugs": {
    "url": "https://github.com/wadewegner/sfdx-waw-plugin/issues"
  },
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/errors": "1",
    "@salesforce/command": "0.1.5",
    "async": "^2.6.1",
    "child_process": "^1.0.2",
    "fs-extra": "^2.0.0",
    "heroku-cli-util": "^6.2.13",
    "jsforce": "^1.8.4",
    "jsonfile": "^2.4.0",
    "node-forge": "^0.7.5",
    "open": "0.0.5",
    "recursive-readdir": "^2.2.2",
    "request": "^2.87.0",
    "salesforce-alm": "^43.2.0",
    "url-exists": "^1.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/plugin-help": "1",
    "@oclif/test": "1",
    "@salesforce/dev-config": "1.0.4",
    "@types/chai": "4",
    "@types/jsforce": "1.8.9",
    "@types/mocha": "5",
    "@types/node": "9",
    "chai": "4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.12.0",
    "globby": "8",
    "mocha": "5",
    "np": "^2.20.1",
    "shelljs": "^0.7.7",
    "sinon": "5",
    "ts-node": "5",
    "tslib": "1",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/WadeWegner/sfdx-waw-plugin",
  "keywords": [
    "force.com",
    "oss",
    "salesforce",
    "salesforce-dx",
    "salesforcedx"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "oclif": {
    "topics": {
      "waw:connectedapp": {
        "description": "manage your Connected Apps"
      },
      "waw:org": {
        "description": "manage your Salesforce DX orgs"
      }
    },
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "WadeWegner/sfdx-waw-plugin",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "release": "np",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
